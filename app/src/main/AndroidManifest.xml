<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raulmartinezperez.ubiqumproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.raulmartinezperez.ubiqumproject.SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.raulmartinezperez.ubiqumproject.AddProduct" />
        <activity android:name="com.raulmartinezperez.ubiqumproject.MainActivity"></activity>
        <activity android:name="com.raulmartinezperez.ubiqumproject.CheckOut"></activity>
        <activity android:name="com.raulmartinezperez.ubiqumproject.Messages"></activity>
    </application>

</manifest>